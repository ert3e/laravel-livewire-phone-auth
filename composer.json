{
    "name": "ert3e/laravel-phone-auth",
    "description": "Laravel+livewire phone auth module",
    "keywords": ["laravel", "livewire", "phone auth", "phone register", "phone login"],
    "type": "library",
    "homepage": "https://github.com/ert3e/laravel-phone-auth",
    "require": {
        "php": "^7.3|^8.0",
        "doctrine/dbal": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.5.x-dev",
        "mockery/mockery": "^1.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitriy Samoylenko"
        }
    ],
    "autoload": {
        "psr-4": {
            "Ert3e\\PhoneAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ert3e\\PhoneAuth\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ert3e\\PhoneAuth\\Providers\\PhoneAuthServiceProvider"
            ]
        }
    }
}
